generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       Float
  area        Float
  address     String
  imageUrls   String[]
  status      String   @default("available")
  ownerId     Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

